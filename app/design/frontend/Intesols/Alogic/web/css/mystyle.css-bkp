     display: block;
     box-sizing: border-box;
    /*    position: absolute;     z-index: 1;     overflow: hidden;*/
     padding:12px 15px 0;
 }
 .block-promo .title { 
     display: block;
     font-weight: normal;
     font-size: 30px;
     line-height: 44px;
     margin-bottom:0px;
     color: #707070;
     font-family: 'proxima_novaregular';
 }

 .block-promo .info {
     bottom: 2px;
     display: block;
    font-size: 23px;
    font-weight: normal;
    padding: 0 15px 12px;
    position: absolute;
    width: 53%;
    line-height: normal;
 }
 .block-promo .more {
     font-size: 1.6rem
 }
