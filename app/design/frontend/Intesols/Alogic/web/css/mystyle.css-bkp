 .block-promo {
     box-sizing: border-box;
     overflow: hidden;
     position: relative;
     display: block;
     width: 100%;
     -webkit-border-radius: 15px;
     -moz-border-radius: 15px;
     -o-border-radius: 15px;
     border-radius: 15px;
 }
 .block-promo:hover {
     text-decoration: none
 }
