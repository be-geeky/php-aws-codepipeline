    border: 1px solid #989898;
}
.browsecatalogs ul li .browsecatalogs_image a.hover_download img {
    border: 0;
    margin: auto;
}

a.hover_download {
    width: 100%;
    text-align: center;
    display: inline-block;
    position: absolute;
    top: 0;
    opacity: 0;
    -webkit-transition: all 0.4s ease-in-out 0s;
    -ms-transition: all 0.4s ease-in-out 0s;
    transition: all 0.4s ease-in-out 0s;
}
.browsecatalogs ul li:hover a.hover_download {
    top: 40%;
    opacity: 1;
}
.browsecatalogs_image{position: relative;}
.browsecatalogs_image:before {
    content: "";
    background: #ffb72a;
    background: rgb(255, 183, 42, 0.5);
    position: absolute;
    height: 100%;
    width: 100%;
    -webkit-transition: all 0.4s ease-in-out 0s;
    -ms-transition: all 0.4s ease-in-out 0s;
    transition: all 0.4s ease-in-out 0s; 
    opacity: 0;   
}

.browsecatalogs ul li:hover .browsecatalogs_image:before {
opacity: 1;
  }
.browsecatalogs {
    width: 100%;
    display: inline-block;
}
